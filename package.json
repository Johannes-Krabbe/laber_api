    "hono": "^4.4.4",
    "i": "^0.3.7",
    "npm": "^10.8.1",
    "prisma": "^5.15.1",
    "prismock": "^1.31.2",
    "typescript-eslint": "^7.12.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@types/bun": "latest",
